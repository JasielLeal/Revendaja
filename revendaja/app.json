{
  "expo": {
    "name": "Revendaja",
    "slug": "revendaja",
    "owner": "szreactioon",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.szreactioon.revendaja"
    },
    "android": {
      "newArchEnabled": true,
      "package": "com.szreactioon.revendaja",
      "permissions": ["NOTIFICATIONS", "CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/frame.png"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f72c3ea6-e4d0-47b8-9ce5-ba720c38e40c"
      }
    },
    "scheme": "revendaja",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/r.png",
          "color": "#ff7100",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ff7100",
          "image": "./assets/r.png",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/r.png",
            "backgroundColor": "#ff7100"
          }
        }
      ]
    ]
  }
}
