{
  "expo": {
    "name": "Revendaja",
    "slug": "revendaja",
    "owner": "szreactioon",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.szreactioon.revendaja"
    },

    "android": {
      "newArchEnabled": true,
      "package": "com.szreactioon.revendaja",
      "permissions": ["NOTIFICATIONS", "CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "extra": {
      "eas": {
        "projectId": "f72c3ea6-e4d0-47b8-9ce5-ba720c38e40c" 
      }
    },

    "scheme": "revendaja",

    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF0000",
          "mode": "production"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FF0000",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Revendaja"
    }
  }
}
